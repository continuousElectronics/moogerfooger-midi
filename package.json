{
    "name": "moogerfooger-midi",
    "version": "0.0.0",
    "description": "Application to control settings via midi on moogerfooger pedals",
    "main": "main.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/continuousElectronics/moogerfooger-midi"
    },
    "scripts": {
        "dev": "concurrently --kill-others \"cross-env NODE_ENV=development webpack\" \"cross-env NODE_ENV=development electron .\"",
        "build": "cross-env NODE_ENV=production webpack",
        "start": "electron .",
        "pack": "electron-builder --dir",
        "dist": "electron-builder",
        "final": "npm run build && npm run dist"
    },
    "build": {
        "appId": "com.electron.moogerfoogermidi",
        "productName": "Moogerfooger Midi",
        "files": [
            "build/bundle.js",
            "build/index.html",
            "main.js"
        ],
        "mac": {
            "category": "music electronics"
        },
        "linux": {
            "target": [
                "AppImage",
                "deb"
            ]
        },
        "win": {
            "target": "squirrel",
            "icon": "build/icon.ico"
        }
    },
    "keywords": [
        "moog",
        "moogerfooger",
        "midi",
        "midi control",
        "music",
        "electronics",
        "analog",
        "electronics"
    ],
    "author": "Will Senisi <will.senisi@gmail.com>",
    "license": "ISC",
    "devDependencies": {
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.2",
        "babel-polyfill": "^6.26.0",
        "babel-preset-env": "^1.6.1",
        "concurrently": "^3.5.1",
        "cross-env": "^5.1.3",
        "css-loader": "^0.28.7",
        "electron": "^1.8.2",
        "electron-builder": "^20.0.7",
        "electron-rebuild": "^1.7.3",
        "electron-reload": "^1.2.2",
        "eslint": "^4.16.0",
        "eslint-plugin-vue": "^4.2.2",
        "file-loader": "^1.1.6",
        "node-sass": "^4.7.2",
        "sass-loader": "^6.0.6",
        "style-loader": "^0.19.0",
        "svg-inline-loader": "^0.8.0",
        "uglifyjs-webpack-plugin": "^1.2.0",
        "url-loader": "^0.6.2",
        "vue-loader": "^13.5.0",
        "vue-template-compiler": "^2.5.9",
        "webpack": "^3.10.0",
        "webpack-dev-server": "^2.9.7"
    },
    "dependencies": {
        "easymidi": "0.0.4",
        "midi-clock": "0.0.1",
        "proto-create": "^1.0.1",
        "vue": "^2.5.9",
        "vue-select": "^2.4.0",
        "webmidi": "^2.0.0"
    }
}
